{
  "name": "next-fe-boilerplate",
  "private": true,
  "packageManager": "bun@latest",
  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "biome lint --reporter=pretty .",
    "format": "biome format --write .",
