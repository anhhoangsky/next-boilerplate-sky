    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@next/bundle-analyzer": "latest",
    "@svgr/webpack": "latest",
    "@next/mdx": "latest",
    "biome": "latest",
    "husky": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "jest": "latest",
    "ts-jest": "latest",
    "@types/jest": "latest",
    "playwright": "latest",
    "typescript": "latest",
    "storybook": "latest",
    "@storybook/nextjs": "latest",
    "@storybook/addon-essentials": "latest"
  }
